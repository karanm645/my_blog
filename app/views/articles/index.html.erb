<h1>My Articles</h1>
<p><%= link_to "Create Article",  new_article_path %></p>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %> </td>
      <td><%= article.description %> </td>
      <td><%= link_to "edit", edit_article_path(article.id)%></td>
      <td><%= link_to "show", "/articles/#{article.id}", method: :get %></td>
    </tr>
  <% end %>
</table>


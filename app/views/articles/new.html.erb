<h1>Create an article</h1>

<%= form_with scope: :articles, url: articles_path, method: :post do |form| %>

  <p>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </p>
  <p>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </p>
# add a button 
  <%= form.submit "Submit" %>
<% end %>